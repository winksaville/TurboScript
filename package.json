{
  "name": "turboscript",
  "version": "1.0.7-alpha",
  "description": "Super charged JavaScript for parallel programming and WebAssembly",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@types/debug": "^0.0.29",
    "debug": "^2.6.3",
    "systemjs": "^0.19.41"
  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "lite-server": "^2.2.2",
    "ts-node": "^1.7.2",
    "typescript": "^2.1.4"
  },
  "bin": {
    "tc": "./bin/tc"
  },
  "scripts": {
    "build": "tsc -p ./src && tsc lib/tc.ts",
    "watch": "tsc -w -p ./src",
    "watch-lib": "tsc -w lib/tc.ts",
    "pretest:build": "mkdir -p ./build/tests",
    "test:build": "./bin/tc tests/addTwo.tbs --out ./build/tests/addTwo.wasm",
    "preserver": "tsc -p ./tests/server/tsconfig.json",
    "server": "DEBUG=server node ./build/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/01alchemist/TurboScript.git"
  },
  "author": "Nidin Vinayakan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/01alchemist/TurboScript/issues"
  },
  "homepage": "https://github.com/01alchemist/TurboScript#readme"
}
